{
	"lintOptions": {
		"typeCheck": true,
    "exclude": [
			"__tests__/**/*.ts",
			"__mocks__/**/*.ts"
    ]
	},
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
	"rules": {
		"no-console": false,
		"object-literal-sort-keys": false,
    "max-classes-per-file": false
	}
}
